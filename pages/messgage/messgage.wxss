page, view {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

page {
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.container {
    display: flex;
    justify-content: center;
    align-items: flex-start;
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    overflow-y: auto;

}

.loading{
    display:inline-block;
    margin-right:12px;
    vertical-align:middle;
    width:14px;
    height:14px;
    background:0 0;
    border-radius:50%;
    border:2px solid #e9eaec;
    border-color:#e9eaec #e9eaec #e9eaec #E6A23C;
    animation:btn-spin .6s linear;
    animation-iteration-count:infinite;
}